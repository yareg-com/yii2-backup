{
  "name": "yareg-com/yii2-backup",
  "description": "Yii2 backup extension",
  "keywords": [
    "yii2",
    "extension",
    "module",
    "backup",
    "zip",
    "gzip"
  ],
  "homepage": "https://github.com/yareg-com/yii2-backup",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "floor12@floor12.net",
      "role": "Developer",
      "homepage": "http://floor12.net"
    },
    {
      "name": "yareg",
      "email": "yar@yareg.com",
      "role": "Developer",
      "homepage": "https://yareg.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "ext-zlib": "*",
    "yiisoft/yii2": "~2.0.19",
    "yii2mod/yii2-enum": "^1.7",
    "ifsnop/mysqldump-php": "2.*",
    "floor12/yii2-notification": "~1.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.5.17"
  },
  "autoload": {
    "psr-4": {
      "yareg\\backup\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "yareg\\backup\\tests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
